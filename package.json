{
  "name": "marketplace",
  "version": "2.32.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Acme-Intergalactic/marketplace-v2"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0 --disable-host-check",
    "start:dev": "ng serve --host=0.0.0.0 --disable-host-check --configuration=dev",
    "start:qa": "ng serve --host=0.0.0.0 --disable-host-check --configuration=qa",
    "bundle-report": "webpack-bundle-analyzer dist/apps/Marketplace/stats-es2015.json",
    "ci:cy-setup": "cypress install",
    "ci:cy-run-record": "start-server-and-test ci:start-server http://localhost:4200 cy:run-record",
    "ci:start-server": "angular-http-server --path ./dist/apps/Marketplace -p 4200 --silent",
    "cy:run-record": "cypress run --record --key $CYPRESS_TOKEN --parallel --group \"Cypress on AWS CodeBuild\" --ci-build-id $COMMIT_INFO_SHA",
    "test:dev": "open -a \"Google Chrome\" coverage/index.html && ng test --watch --code-coverage",
    "test:headless": "ng test --code-coverage",
    "format:check": "nx format:check",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e --protractor-config=e2e/protractor.conf.js --webdriver-update=false",
    "version": "echo $npm_package_version",
    "build:office": "ng build --configuration=office",
    "build:staging": "ng build --configuration=staging",
    "build:production": "node ./replace-version.js && ng build --configuration=production",
    "build:production-ca": "node ./replace-version.js && ng build --configuration=production-ca",
    "build:qa": "ng build --configuration=qa",
    "nx": "nx",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "workspace-schematic": "nx workspace-schematic",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "11.0.1",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "11.0.1",
    "@angular/compiler": "11.0.1",
    "@angular/core": "11.0.1",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "11.0.1",
    "@angular/material": "11.0.0",
    "@angular/material-moment-adapter": "11.0.0",
    "@angular/platform-browser": "11.0.1",
    "@angular/platform-browser-dynamic": "11.0.1",
    "@angular/platform-server": "11.0.1",
    "@angular/router": "11.0.1",
    "@angular/service-worker": "11.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "10.4.4",
    "@nrwl/schematics": "^8.12.11",
    "@swimlane/ngx-charts": "^16.0.0",
    "@types/blueimp-load-image": "^2.23.8",
    "@types/lodash": "^4.14.165",
    "big-integer": "^1.6.48",
    "blueimp-load-image": "^5.14.0",
    "classlist.js": "^1.1.20150312",
    "colorthief": "^2.3.2",
    "core-js": "^3.7.0",
    "crypto-js": "^4.0.0",
    "dom-to-image-font-patch": "^1.0.1",
    "file-saver": "^2.0.5",
    "firebase": "^8.1.2",
    "html2canvas": "^1.0.0-rc.7",
    "lodash": "^4.17.20",
    "material-community-components": "^10.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "ng-intercom": "8.0.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "yarn": "^1.22.4",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular-devkit/schematics": "^11.0.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "11.0.1",
    "@angular/language-service": "^11.0.1",
    "@nrwl/jest": "9.5.1",
    "@nrwl/workspace": "^10.4.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.8",
    "@types/crypto-js": "^4.0.1",
    "@types/fabric": "^3.6.9",
    "@types/file-saver": "^2.0.1",
    "@types/googlemaps": "^3.40.3",
    "@types/jest": "25.1.4",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.14.8",
    "angular-http-server": "^1.9.0",
    "codelyzer": "^6.0.1",
    "cypress": "^5.6.0",
    "firebase-tools": "^8.16.2",
    "husky": "^4.3.0",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "ng-bullet": "^1.0.3",
    "ng-mocks": "^11.0.0",
    "prettier": "2.1.2",
    "pretty-quick": "^3.1.0",
    "replace-in-file": "^6.1.0",
    "semantic-release": "^17.2.3",
    "source-map-explorer": "^2.5.0",
    "start-server-and-test": "^1.11.5",
    "ts-jest": "25.2.1",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.1.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --fix"
    }
  }
}
